=============================================================================================================================
GET METHODS

let a = new Date( );

console.log(a.getFullYear()); //2026

console.log(a.getMonth()); //0 from jan to dec it will return the numbers from 0 to 11.

console.log(a.getDate());  //7 todays date is returned.

console.log(a.getHours()); // 16  the hour is returned here.

console.log(a.getMinutes()); //14 minutes will be write here.

console.log(a.getSeconds()); //50 seconds will be returned here.

console.log(a.getMilliseconds()); //from 0 to 999 milliseconds will be counted.

console.log(a.getDay());  // 0 to 6 days will be printed.

let b = Date.now();
console.log(b);

===============================================================================================================================

SET METHODS

const a = new Date();
a.setFullYear(2025,11,6);
console.log(a);

a.setDate(23);
console.log(a);

a.setMonth(2);
console.log(a);

a.setHours(12);
console.log(a);

a.setMinutes(12);
console.log(a);

a.setSeconds(12);
console.log(a);

a.setMilliseconds(12);
console.log(a);


<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<Set time out function>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
function wish(){
    console.log("Good Evening!!");
}
setTimeout(wish,2000);

<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<clear time out function>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>

//clear time out function

function write(){
    console.log("congrats!!");
}

let a1 = setTimeout(write, 3000);

function clear(){
    clearTimeout(a1);
}

