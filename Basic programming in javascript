Sum of numbers
let N =20;
let sum=0;
for(let i=0; i<N ;i++){
   sum+=i;   
}
console.log(sum);
==========================================================================
//even 1 to 50
let num =50;
for(let i=1; i<=num;i++){
    if(i%2 == 0){
        console.log(i);
    }
    
}
==========================================================================
// number is aven or odd
let num=9;
//console.log(num%2 == 0? num + " is a even number" : num+" is a odd number");

if(num%2 == 0){
    console.log(num+" is a even number");
}else{
    console.log(num+ " is aodd number");
}
=========================================================================
//Larger number between two numbers
let num1 = 49;
let num2 = 300;
if(num1>num2){
    console.log(num1 + " is Largest number");
}else{
    console.log(num2+ " is a Largest number");
}
===============================================================================
//multiplication of a table
let num =2;
for(let i=0;i<=10;i++){
    res= num * i;
    console.log(num+ " * "+ i +" = "+res);
}
===============================================================================
//printing 1 to 50 numbers
let N=50;

for(let i=1; i<=N; i++){
    console.log(i);
}
===============================================================================
// printing odd numbers
let N=100;
for(let i=0;i<N;i++){
    if(i+=1){
        console.log(i);
    }
    
}
===============================================================================
// Given number is positive or odd number
let num=-4;
if(num>0){
    console.log(num+" is a positive number.");
}else if(num==0){
    console.log(num+" is equal to zero.");
}else{
    console.log(num+" is a negative number.");
}
